    <!-- 主activity-->
    <mainactivity>com.xiaomi.smarthome/.SmartHomeMainActivity</mainactivity>
    <!-- activity白名单-->
    <whiteactivity>
        com.xiaomi.smarthome/.SmartHomeMainActivity,
        com.xiaomi.smarthome/.frame.plugin.runtime.activity.PluginHostActivity,
        com.xiaomi.smarthome/.frame.plugin.runtime.activity.PluginHostActivityPlugin1,
